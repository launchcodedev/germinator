{
  "name": "@servall/germinator",
  "version": "0.0.3",
  "description": "A package to help with seeding postgres databases using yaml",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "author": {
    "name": "Servall Development",
    "email": "admin@servalldev.com",
    "url": "https://www.servalldev.com"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "cli": "ts-node src/cli.ts",
    "coverage": "jest --coverage",
    "lint": "yarn lint:ts",
    "fix": "yarn fix:ts",
    "lint:ts": "tslint --project tsconfig.json src/**/*.ts src/**/*.tsx",
    "fix:ts": "yarn lint:ts --fix",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "bcrypt": "^3.0.1",
    "fs-extra": "^7.0.0",
    "js-yaml": "^3.12.0",
    "klaw": "^3.0.0",
    "through2": "^2.0.3"
  },
  "peerDependencies": {
    "typeorm": "0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.5",
    "@types/js-yaml": "^3.11.2",
    "@types/klaw": "^2.1.1",
    "@types/node": "^10.12.1",
    "@types/through2": "^2.0.34",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^3",
    "tslint": "^5",
    "tslint-config-airbnb": "^5",
    "tslint-eslint-rules": "^5",
    "typescript": "^3"
  },
  "jest": {
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  }
}
