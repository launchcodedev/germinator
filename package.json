{
  "name": "@servall/germinator",
  "version": "0.3.5-dev",
  "description": "A package to help with seeding databases using declarative seeds",
  "main": "dist/index.js",
  "types": "dist-types/index.d.ts",
  "license": "UNLICENSED",
  "author": {
    "name": "Servall Development",
    "email": "admin@servalldev.com",
    "url": "https://www.servalldev.com"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "dev lint",
    "fix": "dev lint --fix",
    "test": "jest",
    "knex": "knex --knexfile knexfile.ts",
    "migrate:make": "yarn knex migrate:make -x ts",
    "migrate:latest": "yarn knex migrate:latest",
    "migrate:rollback": "yarn knex migrate:rollback",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "peerDependencies": {
    "@servall/logger": "0.1"
  },
  "dependencies": {
    "@servall/mapper": "0.1",
    "@servall/ts": "0.2",
    "ajv": "6",
    "bcrypt": "3",
    "chance": "1",
    "faker": "4",
    "fs-extra": "8",
    "handlebars": "4",
    "handlebars-helper-repeat-root-fixed": "2",
    "handlebars-helpers": "0.10",
    "hogan.js": "3",
    "js-yaml": "3",
    "knex": "0.20",
    "koa": "2",
    "lodash": "4",
    "moment": "2",
    "object-hash": "1",
    "pg": "7",
    "to-snake-case": "1"
  },
  "devDependencies": {
    "@commitlint/cli": "8",
    "@servall/commitlint": "1",
    "@servall/dev": "0.0.10-beta.13",
    "@servall/eslint-config": "0.1",
    "@servall/jest": "0.1",
    "@servall/logger": "0.1",
    "@servall/prettier": "0.1",
    "@servall/tsconfig": "0.1",
    "@types/bcrypt": "3",
    "@types/chance": "1",
    "@types/faker": "4",
    "@types/fs-extra": "8",
    "@types/hogan.js": "3",
    "@types/jest": "24",
    "@types/js-yaml": "3",
    "@types/koa": "2",
    "@types/lodash": "4",
    "@types/node": "12",
    "@types/object-hash": "1",
    "@types/to-snake-case": "1",
    "husky": "3",
    "jest": "24",
    "sqlite3": "4",
    "ts-node": "8",
    "typescript": "3"
  },
  "prettier": "@servall/prettier",
  "eslintConfig": {
    "extends": "@servall"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@servall/commitlint"
    ]
  },
  "jest": {
    "preset": "@servall/jest"
  }
}
