{
  "name": "@servall/germinator",
  "version": "0.2.0",
  "description": "A package to help with seeding databases using declarative seeds",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "license": "UNLICENSED",
  "author": {
    "name": "Servall Development",
    "email": "admin@servalldev.com",
    "url": "https://www.servalldev.com"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "fix": "yarn lint --fix",
    "knex": "knex --knexfile knexfile.ts",
    "migrate:make": "yarn knex migrate:make -x ts",
    "migrate:latest": "yarn knex migrate:latest",
    "migrate:rollback": "yarn knex migrate:rollback",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@servall/logger": "0.1",
    "@servall/mapper": "0.1",
    "@servall/ts": "0.2",
    "ajv": "6",
    "bcrypt": "3",
    "chance": "1",
    "faker": "4",
    "fs-extra": "7",
    "handlebars": "4",
    "handlebars-helper-repeat-root-fixed": "2",
    "handlebars-helpers": "0.10",
    "hogan.js": "3",
    "js-yaml": "3",
    "knex": "0.17",
    "koa": "2",
    "lodash": "4",
    "moment": "2",
    "object-hash": "1",
    "pg": "7",
    "to-snake-case": "1"
  },
  "devDependencies": {
    "@types/bcrypt": "3",
    "@types/chance": "1",
    "@types/faker": "4",
    "@types/fs-extra": "7",
    "@types/hogan.js": "3",
    "@types/jest": "24",
    "@types/js-yaml": "3",
    "@types/koa": "2",
    "@types/lodash": "4",
    "@types/moment": "2",
    "@types/node": "10",
    "@types/object-hash": "1",
    "@types/to-snake-case": "1",
    "jest": "24",
    "sqlite3": "4",
    "ts-jest": "24",
    "ts-node": "8",
    "tslint": "5",
    "tslint-config-airbnb": "5",
    "tslint-eslint-rules": "5",
    "typescript": "3"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": null,
    "testRegex": "(tests/.*|src/.*\\.test)\\.(ts|js)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".yarn-cache"
    ],
    "modulePathIgnorePatterns": [
      "node_modules",
      ".yarn-cache"
    ]
  }
}
