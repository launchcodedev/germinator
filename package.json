{
  "name": "@servall/germinator",
  "version": "0.2.0",
  "description": "A package to help with seeding postgres databases using yaml",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "author": {
    "name": "Servall Development",
    "email": "admin@servalldev.com",
    "url": "https://www.servalldev.com"
  },
  "bin": {
    "germinator": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "cli": "ts-node src/cli.ts",
    "test": "jest",
    "lint": "yarn lint:ts",
    "fix": "yarn fix:ts",
    "lint:ts": "tslint src/**/*.ts src/**/*.tsx",
    "fix:ts": "yarn lint:ts --fix",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "bcrypt": "3",
    "fs-extra": "7",
    "glob": "7",
    "glob-promise": "3",
    "js-yaml": "3",
    "pg": "7",
    "yargs": "12"
  },
  "peerDependencies": {
    "typeorm": "0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "3",
    "@types/fs-extra": "5",
    "@types/jest": "23",
    "@types/js-yaml": "3",
    "@types/node": "10",
    "@types/yargs": "12",
    "jest": "23",
    "sqlite3": "4",
    "ts-jest": "23",
    "ts-node": "3",
    "tslint": "5",
    "tslint-config-airbnb": "5",
    "tslint-eslint-rules": "5",
    "typeorm": "0.2",
    "typescript": "3"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": null,
    "testRegex": "(tests/.*|src/.*\\.test)\\.(ts|js)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".yarn-cache"
    ],
    "modulePathIgnorePatterns": [
      "node_modules",
      ".yarn-cache"
    ]
  }
}
