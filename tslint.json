{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rulesDirectory": [
    "tslint-eslint-rules"
  ],
  "rules": {
    "no-increment-decrement": false,
    "no-unused-expression": true,
    "import-name": false,
    "ter-padded-blocks": [
      true,
      "never"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "align": [
      true,
      "parameters",
      "statements",
      "members",
      "elements"
    ]
  }
}
