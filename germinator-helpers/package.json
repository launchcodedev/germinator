{
  "name": "@germinator/helpers",
  "description": "Dynamic database seeds using declarative YAML files",
  "version": "0.4.7",
  "license": "MPL-2.0",
  "author": {
    "name": "Launchcode",
    "email": "admin@lc.dev",
    "url": "https://lc.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchcodedev/germinator.git"
  },
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "!*.tsbuildinfo",
    "!*.test.*"
  ],
  "scripts": {
    "build": "tsc -b",
    "build:es": "tsc -b tsconfig.es.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "eslint src",
    "fix": "eslint --fix src",
    "test": "TZ=utc jest",
    "prepublishOnly": "yarn clean && yarn build && yarn build:es"
  },
  "dependencies": {
    "@germinator/core": "0.4",
    "bcrypt": "5",
    "chance": "1",
    "faker": "4",
    "handlebars-helper-repeat-root-fixed": "2",
    "handlebars-helpers": "0.10",
    "js-yaml": "4",
    "lodash.get": "4",
    "moment": "2"
  },
  "devDependencies": {
    "@types/bcrypt": "3",
    "@types/chance": "1",
    "@types/common-tags": "1",
    "@types/faker": "4",
    "@types/lodash.get": "4",
    "common-tags": "1"
  },
  "prettier": "@lcdev/prettier",
  "jest": {
    "preset": "@lcdev/jest"
  }
}
